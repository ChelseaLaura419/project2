                <form>
                    <div class="form-group">
                        <label for="characterName">Character Name</label>
                        <input type="text" class="form-control" id="characterName" placeholder="Character Name">
                    </div>
                    <div class="form-group">
                        <label for="characterClass">Class</label>
                        <input type="text" class="form-control" id="characterClass" placeholder="Class">
                    </div>
                    <div class="form-group">
                        <label for="characterAge">Age</label>
                        <input type="text" class="form-control" id="characterAge" placeholder="Age">
                    </div>
                    <div class="form-group">
                        <label for="characterHeight">Height</label>
                        <input type="text" class="form-control" id="characterHeight" placeholder="Height">
                    </div>
                    <div class="form-group">
                        <label for="characterStrength">Strength</label>
                        <input type="text" class="form-control" id="characterStrength" placeholder="Strength">
                    </div>
                    <div class="form-group">
                        <label for="characterWeakness">Weakness</label>
                        <input type="text" class="form-control" id="characterWeakness" placeholder="Weakness">
                    </div>
                    <div class="form-group">
                        <label for="characterDescription">Description</label>
                        <input type="text" class="form-control" id="characterDescription" placeholder="Description">
                    </div>
                    <div class="form-group">
                        <label for="characterPhoto">Character Photo</label>
                        <input type="text" class="form-control" id="characterPhoto" placeholder="Image URL">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
